<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<META HTTP-EQUIV="Pragma" CONTENT="no-cache">
@if?!Authenticate@ @AddFile?/grinder/failedlogin.cmvp@ @else@
@if?!CheckAuthCode?AUTH_CMDRACES=true@ @AddFile?/grinder/nopriv.cmvp@ @else@
<html>
	<META CHARSET="utf-8"/>
<head>
    <link rel="grinder icon" href="/images/grinder.ico" >
    <link rel="STYLESHEET" TYPE="TEXT/CSS" HREF="grinder.css">
</head>
<body BGCOLOR=BLACK>
<SCRIPT LANGUAGE=JavaScript>
//<!--
@if?CheckReqParm?FUNCTION=DELRACE@
    var v='@MUDGrinder?DELRACE@';
    if((v != '')&&(alert))
        alert(v);
    else
    if(alert)
        alert('Race deleted.');
    opener.Refresh();
    window.close();
@endif@
@if?CheckReqParm?FUNCTION=EDITRACE@
    var v='@MUDGrinder?EDITRACE@';
    if((v != '')&&(alert))
        alert(v);
    else
    if(alert)
        alert('Race created/modified.');
    opener.Refresh();
    window.close();
@endif@
@if?CheckReqParm?FUNCTION=RERACE@
    @if?CheckReqParm?NEWRACEID=@
        document.location.href='race.cmvp?RACE=@RaceID@';
    @else@
        document.location.href='race.cmvp?RACE=@RequestParameterEncoded?NEWRACEID@&NEWRACE=@RequestParameterEncoded?NEWRACEID@';
    @endif@
@endif@
//-->
</SCRIPT>

</BODY>
</HTML>
@endif@
@endif@
