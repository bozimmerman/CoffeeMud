<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<META HTTP-EQUIV="Pragma" CONTENT="no-cache">
@if?!Authenticate@ @AddFile?/grinder/failedlogin.cmvp@ @else@
@if?!CheckAuthCode?AUTH_CMDSOCIALS=true@ @AddFile?/grinder/nopriv.cmvp@ @else@
<html>
	<META CHARSET="utf-8"/>
<head>
    <link rel="grinder icon" href="/images/grinder.ico" >
    <link rel="STYLESHEET" TYPE="TEXT/CSS" HREF="grinder.css">
</head>
<body BGCOLOR=BLACK>
<SCRIPT LANGUAGE=JavaScript>
<!--
@if?CheckReqParm?FUNCTION=DELSOCIAL@
    var v='@SocialData?DELETE@';
    if((v != '')&&(alert))
        alert(v);
    else
    if(alert)
        alert('Social deleted.');
    opener.Refresh();
    window.close();
@endif@
@if?CheckReqParm?FUNCTION=EDITSOCIAL@
    var v='@SocialData?EDIT@';
    if((v != '')&&(alert))
        alert(v);
    else
    if(alert)
    {
        @if?CheckReqParm?COMPONENT=true@
            alert('Social created/modified.');
        @else@
	        alert('Component Social created/modified.');
        @endif@
    }
    opener.Refresh();
    window.close();
@endif@
@if?CheckReqParm?FUNCTION=RESOCIAL@
    @if?CheckReqParm?NEWSOCIALID=@
        document.location.href='social.cmvp?SOCIAL=@RequestParameterEncoded?SOCIAL@';
    @else@
        document.location.href='social.cmvp?SOCIAL=@RequestParameterEncoded?NEWSOCIALID@';
    @endif@
@endif@
//-->
</SCRIPT>

</BODY>
</HTML>
@endif@
@endif@
