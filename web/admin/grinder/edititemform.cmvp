			@ItemData?RECIPEDATA&RECIPEPREFIX=<INPUT TYPE=TEXT NAME=RECIPEDATA### SIZE=60  MAXLENGTH=255 VALUE="&RECIPEPOSTFIX="><BR>&RECIPEFIELDNAME=RECIPEDATA###@
		</TD>
	</TR>
	@endif@
	@if?!ItemData?ISCOIN@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Base Gold Value:</B></FONT></TD>
