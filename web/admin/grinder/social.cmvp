                    document.RESOURCES.FUNCTION.value='EDITSOCIAL';
                    document.RESOURCES.submit();
                }
        }
        function ReShow()
        {
            document.RESOURCES.FUNCTION.value='';
            document.RESOURCES.action='/grinder/social.cmvp';
