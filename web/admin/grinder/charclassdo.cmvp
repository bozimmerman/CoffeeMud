@if?CheckReqParm?FUNCTION=DELCLASS@
    var v='@MUDGrinder?DELCLASS@';
    if((v != '')&&(alert))
        alert(v);
    else
    if(alert)
        alert('Class deleted.');
    opener.Refresh();
    window.close();
@endif@
@if?CheckReqParm?FUNCTION=EDITCLASS@
    var v='@MUDGrinder?EDITCLASS@';
    if((v != '')&&(alert))
        alert(v);
    else
    if(alert)
        alert('Class created/modified.');
    opener.Refresh();
    window.close();
@endif@
@if?CheckReqParm?FUNCTION=RECLASS@
