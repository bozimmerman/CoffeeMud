        document.location.href='charclass.cmvp?CLASS=@CharClassID@';
    @else@
        document.location.href='charclass.cmvp?CLASS=@RequestParameterEncoded?NEWCLASSID@&NEWCLASS=@RequestParameterEncoded?NEWCLASSID@';
    @endif@
@endif@
//-->
</SCRIPT>

</BODY>
</HTML>
